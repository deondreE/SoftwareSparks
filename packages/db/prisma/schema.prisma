generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model CUSTOMERS {
  ID      Int      @id
  NAME    String   @db.VarChar(20)
  AGE     Int
  ADDRESS String?  @db.Char(25)
  SALARY  Decimal? @db.Decimal(18, 2)
}

model Teachers {
  ID      Int      @id
  NAME    String   @db.VarChar(20)
  AGE     Int
  ADDRESS String?  @db.Char(25)
  SALARY  Decimal? @db.Decimal(18, 2)
}

model Students {
  ID      Int      @id
  NAME    String   @db.VarChar(20)
  AGE     Int
  ADDRESS String?  @db.Char(25)
  SALARY  Decimal? @db.Decimal(18, 2)
}

model Class {
  ID      Int      @id
  NAME    String   @db.VarChar(20)
} 